{
  "_id": "{{@ system @}}",
  "headers": {
    "Ocp-Apim-Subscription-Key": "$SECRET(subscription_key)",
    "tb-key": "$SECRET(api_key)",
    "x-api-version": "3.0"
  },
  "operations": {
    "ansatt-list": {
      "id_expression": "{{ Id }}",
      "method": "GET",
      "payload_property": "value",
      "url": "ansatt/ansatt?$select=*&$top=10"
    }
  },
  "type": "system:rest",
  "url_pattern": "https://api.tidsbanken.net/%s",
  "verify_ssl": true
}
